# Gwet A Nuwumbwer
pwint("Enter thew nuwumbwer: ")
pwease nuwu = inpwt_int()

# FUNCTION to Cawculate Fibwonacchi Nuwumbwer OwO
FUNCTION fib_owo(nuwu) ->
    IF nuwu <= 0 THWEN
        WETURN 0
    ELIF nuwu == 1 THWEN
        WETURN 1
    ELSE
        WETURN fib_owo(nuwu-1) + fib_owo(nuwu-2)

# Pwint the Answew OwO
pwease nwani = fib_owo(nuwu)
pwint("The fibwonacchi nuwumbwer is: ", nwani, " OwO")
